{
  "name": "Maps",
  "version": "1.0.0",
  "description": "A library for working with maps (Based on MapBox-js and Turf-js)",
  "main": "index.js",
  "repository": "https://github.com/elyash14/maps",
  "author": "Elyas",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "parcel index.html --open",
    "build": "yarn clean && parcel build index.html -d dist --public-url ./ --no-source-maps",
    "clean": "yarn clean:cache && yarn clean:dist",
    "clean:cache": "rimraf .cache",
    "clean:dist": "rimraf dist"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "babel-preset-env": "^1.7.0",
    "parcel-bundler": "^1.12.4",
    "sass": "^1.26.3"
  },
  "dependencies": {
    "@mapbox/mapbox-gl-draw": "^1.1.2",
    "@turf/turf": "^5.1.6",
    "mapbox-gl": ">=0.27.0 <2.0.0",
    "sweetalert": "^2.1.2"
  }
}
